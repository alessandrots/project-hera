<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- 
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
   version="2.5">
    -->
<!DOCTYPE web-app
        PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>   

  <display-name>Alessandro Servlet</display-name>
  <description>
     Test Servlet
  </description>
  
  <servlet>
    <servlet-name>newproject</servlet-name>
    <servlet-class>com.outline.org.core.server.ProjectServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    	<servlet-name>newproject</servlet-name>
        <url-pattern>/newproject/*</url-pattern>
  </servlet-mapping>
  
  <listener>
		<listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
	</listener>
	
	<context-param>
		<param-name>contextConfigLocation</param-name>
		<param-value>classpath:/module-core-spring.xml</param-value>
	</context-param>
  
  <!-- The Welcome File List
    <welcome-file-list>        
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
  -->


</web-app>
